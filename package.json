{
  "name": "quick-snippets",
  "displayName": "Quick Snippets",
  "description": "Start creating snippets quickly and easily.ðŸš€",
  "version": "0.0.1",
  "publisher": "Mubashar-Javed",
  "icon": "assets/images/qs-icon.png",
  "engines": {
    "vscode": "^1.72.0"
  },
  "keywords": [
    "snippets",
    "quick snippets",
    "quick-snippets",
    "quick snippets vscode",
    "snippet generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Mubashar-javed/quick-snippets"
  },
  "homepage": "https://github.com/Mubashar-javed/quick-snippets",
  "author": {
    "name": "Mubashar Javed",
    "email": "mubasharbravo302@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Mubashar-javed/quick-snippets/issues"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "quick-snippets.createSnippet"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-snippets.createSnippet",
        "title": "Quick Snippets: Create Snippet"
      }
    ],
    "keybindings": [
      {
        "command": "quick-snippets.createSnippet",
        "key": "ctrl+alt+q",
        "mac": "cmd+shift+q"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format": "npm run lint && prettier --write . && git add .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "husky": "^8.0.2",
    "mocha": "^10.1.0",
    "prettier": "^2.8.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
